{
  "name": "nmbxd",
  "displayName": "x岛匿名版 xdnmb",
  "description": "x岛匿名版 vscode插件",
  "publisher" : "shiyu15",
  "version": "0.0.1",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nmbxd.login",
        "title": "登录"
      },
      {
        "command": "nmbxd.addCollectId",
        "title": "添加订阅ID"
      },
      {
        "command": "nmbxd.forumItemClick",
        "title": "打开分论坛"
      },
      {
        "command": "nmbxd.topicClick",
        "title": "打开帖子"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "nmbxd.login",
          "when": "view==nmbxdForumList"
        },
        {
          "command": "nmbxd.addCollectId",
          "when": "view==nmbxdForumList"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "forum",
          "title": "匿名版x岛",
          "icon": "./images/icon.svg"
        }
      ]
    },
    "views": {
      "forum": [
        {
          "id": "nmbxdForumList",
          "name": "版面列表"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "less": "^4.2.2",
    "request": "^2.88.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint-parser": "^22.0.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "eta": "^3.5.0",
    "form-data": "^4.0.0",
    "path": "^0.12.7",
    "jsdom": "^25.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shiyu15/nmbxd_vscode.git"
  }
}
